<form method="GET" action="/mounts">
    <label for="search">Search mounts:</label>
    <input id="search" type="text" name="search" />
    <button type="submit">Search</button>
</form>

<ul>
    <% mounts.forEach(mount => { %>
        <li>
            <img src="<%= mount.icon %>" />

            <a href="/mounts/<%= mount.id %>"><%= mount.name %></a>
        </li>
    <% }) %>
</ul>